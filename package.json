{
  "name": "conduit-api",
  "version": "0.1.13",
  "main": "index.js",
  "repository": "git@github.com:fdaciuk/conduit-api.git",
  "author": "Fernando Daciuk - @fdaciuk",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "node dist/src/index.js",
    "dev": "docker-compose down && docker-compose up -d && yarn server",
    "server": "nodemon --exec sucrase-node src/index.ts",
    "clean:db": "docker-compose down --volumes --remove-orphans",
    "test": "jest --passWithNoTests && yarn test:integration",
    "test:watch": "jest --watch",
    "test:integration": "jest --passWithNoTests --config jest.config.integration.js",
    "test:integration:watch": "yarn test:integration --watch",
    "test:api": "APIURL=http://localhost:3333/api ./src/config/tests/postman/run-api-tests.sh",
    "lint": "eslint . --ext js,ts",
    "lint:fix": "eslint --fix . --ext js,ts",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "prepare": "husky install",
    "build": "tsc",
    "ci": "yarn lint && yarn type-check && yarn test"
  },
  "dependencies": {
    "argon2": "0.28.2",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "fastify": "3.20.1",
    "fastify-cors": "6.0.2",
    "fp-ts": "2.9.5",
    "io-ts": "2.2.16",
    "io-ts-types": "0.5.16",
    "jose": "3.14.0",
    "module-alias": "2.2.2",
    "monocle-ts": "2.3.10",
    "newtype-ts": "0.3.4",
    "slugify": "1.5.3",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@types/cors": "2.8.12",
    "@types/express": "4.17.12",
    "@types/jest": "26.0.22",
    "@types/module-alias": "2.0.0",
    "@types/node": "14.14.41",
    "@types/uuid": "8.3.1",
    "@typescript-eslint/eslint-plugin": "4.21.0",
    "@typescript-eslint/parser": "4.21.0",
    "eslint": "^7.12.1",
    "eslint-config-standard": "16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "24.3.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "husky": "6.0.0",
    "jest": "26.6.3",
    "nodemon": "2.0.7",
    "sucrase": "3.17.1",
    "ts-jest": "26.5.4",
    "typescript": "4.2.4"
  }
}
